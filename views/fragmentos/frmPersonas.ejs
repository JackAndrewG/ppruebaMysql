
<meta charset="utf-8">
<title>
  <%= titulo %>
</title>
<!--<script src="javascripts/paises.js"></script> -->
  <div class="gallery"><h1>PERSONAS REGISTRADAS</h1></div>
<div class="container">
  <div class="row">
    <!--  LIST-->
    <div class="col-md-7">
      <div style="background-color:green; color:white; font-size: 24px;">
        <%- locals.flash %>
      </div>
      <table class="table table-bordered table-hover">
        <thead>
          <tr>
            <th>CEDULA</th>
            <th>NOMBRE</th>
            <th>APELLIDO</th>
            <th>DIRECCION</th>
            <th>TELEFONO</th>
          </tr>
        </thead>
        <tbody>
         <% listado.forEach(function(persona, i){%>
          <tr>
            <td><%= persona.cedula%></td>
            <td><%= persona.nombre%></td>
            <td><%= persona.apellido%></td>
            <td><%= persona.direccion%></td>
            <td><%= persona.telefono%></td>
          </tr>
          <%}); %>
        </tbody>
      </table>
    </div>
  </div>
</div>
